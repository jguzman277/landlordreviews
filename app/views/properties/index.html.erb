<h1>Properties</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th scope="col">Street Address</th>
      <th scope="col">City</th>
      <th scope="col">State</th>
      <th scope="col">Country</th>
      <th scope="col">Zip Code</th>
    </tr>
  </thead>
  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td scope="col"><%= link_to property.street_address, property %></td>
        <td scope="col"><%= property.city %></td>
        <td scope="col"><%= property.state.state %></td>
        <td scope="col"><%= property.country.country %></td>
        <td scope="col"><%= property.zip_code %></td>
      </tr>
    <% end %>
  </tbody>
  
</table>

<%= link_to "New property", new_property_path, class: "btn btn-primary mt-3" %>
